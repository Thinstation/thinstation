PATH=/bin:/usr/bin:/sbin:/usr/sbin:/usr/local/bin:/usr/local/sbin:/lib/udev:/lib/bin:/fastboot/bin
HOME=/root
XAPPLRESDIR=/lib/X11/app-defaults/
LOGFILE=/var/log/boot.log
