name=pyodbc
version=4.0.32
release=1

build() {
#source=(https://files.pythonhosted.org/packages/d1/21/755f5fa373d01d1803d992c7611799dc4e9fcbb5db7d0852990d6ab3c9ad/pyodbc-$version.tar.gz)
	tar -xzf pyodbc-$version

	cd pyodbc-$version

	python3 ./setup.py install --root $PKG
}
