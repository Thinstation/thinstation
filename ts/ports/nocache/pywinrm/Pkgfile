name=pywinrm
version=0.4.2
release=1

build() {

#source=(  https://files.pythonhosted.org/packages/a6/2c/b43e69d5f8938f443e369a32b4a70c0874bb4b52cd2e378107f3eddb8cf9/pywinrm-0.4.2.tar.gz)

	tar -xzf pywinrm-$version
	cd pywinrm-$version
	python3 ./setup.py install --root $PKG
}
