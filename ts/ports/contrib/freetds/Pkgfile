name=freetds 
version=1.3.3
release=1


source=( ftp://ftp.freetds.org/pub/freetds/stable/freetds-1.3.3.tar.gz) 

build() {
	cd freetds-$version

	./configure 

	make
	make DESTDIR=$PKG install
}
