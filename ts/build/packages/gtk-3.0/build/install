#!/bin/sh

export PACKAGE=gtk-3.0
export PORTS="gtk3 dconf"
export DROP_FILES="gtk-launch gtk-query-immodules-3.0 gtk3-demo gtk3-demo-application gtk3-widget-factory gsettings-data-convert gtk-builder-tool gtk-encode-symbolic-svg gtk3-icon-browser broadwayd"
export DROP_DIRS=lib/girepository-1.0
repackage -e

returnval=$?

exit $returnval
