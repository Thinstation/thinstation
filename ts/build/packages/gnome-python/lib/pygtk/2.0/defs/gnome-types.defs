;; -*- scheme -*-

(define-boxed GnomeModuleInfo
  (in-module "Gnome")
  (c-name GnomeModuleInfo)
  (gtype-id "GNOME_TYPE_MODULE_INFO")
  (copy-func "gnome_module_info_copy")
  (release-func "gnome_module_info_free")
  (fields
   '("const-gchar*" "name")
   '("const-gchar*" "version")
   '("const-gchar*" "description")
  )
)
